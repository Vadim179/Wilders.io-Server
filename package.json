{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development ts-node src/main.ts",
    "dev:watch": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/main.ts",
