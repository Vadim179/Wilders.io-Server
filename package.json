    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "matter-js": "^0.18.0",
    "morgan": "^1.10.0",
    "msgpack-lite": "^0.1.26",
    "reflect-metadata": "^0.2.1",
    "socket.io": "^4.5.3",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/helmet": "^4.0.0",
    "@types/jsdom": "^20.0.0",
    "@types/matter-js": "^0.18.2",
    "@types/morgan": "^1.9.3",
    "@types/msgpack-lite": "^0.1.11",
    "@types/node": "^18.11.0",
    "@types/socket.io": "^3.0.2",
    "@types/ws": "^8.5.10",
    "colors": "^1.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-google": "^0.14.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.8.4"
  }
